<?xml version="1.0" encoding="UTF-8"?>
<interface>
	<object class="GtkApplicationWindow" id="mainWindow">
		<property name="title">gtk4.openssl.interface</property>
		<property name="default-width">640</property>
		<property name="default-height">480</property>
		<property name="resizable">FALSE</property>
		<style><class name="mainWindow"/></style>
		<child>
			<object class="GtkBox" id="programBox">
				<property name="orientation">GTK_ORIENTATION_HORIZONTAL</property>
				<property name="spacing">0</property>
				<style><class name="programBox"/></style>
				<child>
					<object class="GtkNotebook" id="functionsGroupNotepad">
						<property name="tab-pos">GTK_POS_LEFT</property>
						<property name="scrollable">TRUE</property>
						<property name="show-border">False</property>
						<style><class name="functionsGroupNotepad"/></style>
					</object>
				</child>
			</object>
		</child>
	</object>
</interface>